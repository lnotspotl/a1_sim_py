<launch>

    <!-- Joystick node -->
    <node pkg="joy" type="joy_node" name="JOYSTICK">
        <param name="joy_node/dev" value="/dev/input/js1"/>
    </node>

    <!-- Ramped Joystick node -->
    <node pkg="a1_joystick" type="ramped_joystick.py"
        name="RAMPED_JOYSTICK" output="screen"/>

</launch>
